"*":
  "atom-clock":
    dateFormat: "HH:mm"
  "atom-material-ui":
    colors:
      abaseColor: "#2196f3"
      predefinedColor: "Blue"
    fonts:
      fontSize: 14
    tabs:
      compactTabs: true
    treeView:
      compactList: true
    ui:
      panelContrast: true
      panelShadows: true
  "autocomplete-plus":
    autoActivationDelay: 250
    enableAutoActivation: false
    maxVisibleSuggestions: 15
  blame:
    defaultWidth: 500
  core:
    autoHideMenuBar: true
    automaticallyUpdate: false
    closeDeletedFileTabs: true
    disabledPackages: [
      "metrics"
      "whitespace"
      "php-twig"
      "language-blueberry"
      "image-view"
      "language-c"
      "language-clojure"
      "language-csharp"
      "language-mustache"
      "language-make"
      "language-objective-c"
      "language-perl"
      "language-ruby"
      "language-ruby-on-rails"
      "language-toml"
      "language-xml"
      "link"
      "open-on-github"
      "package-generator"
      "styleguide"
      "update-package-dependencies"
      "welcome"
      "github"
      "atom-clock"
      "file-watcher"
      "wakatime"
      "file-icons"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "node_modules"
      "storage/**/*"
      ".idea"
    ]
    packagesWithKeymapsDisabled: [
      "center-cursor"
    ]
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "atom-material-syntax-dark"
    ]
    useCustomTitleBar: false
  docblockr:
    per_section_indent: true
  editor:
    fontFamily: "Fira Code"
    fontSize: 15
    lineHeight: 2.05
    scrollPastEnd: true
    showIndentGuide: true
    tabLength: 4
  "fast-eslint":
    grammarScopes: [
      "source.js"
      "source.jsx"
      "source.es6"
      "source.js.jsx"
      "source.babel"
      "source.js-semantic"
      "source.vue"
    ]
  "file-watcher":
    autoReload: true
  "fuzzy-finder":
    ignoredNames: [
      ".git"
      "._*"
      "node_modules"
      "storage/**/*"
      ".idea"
    ]
    preserveLastSearch: true
  hyperclick:
    linuxTriggerKeys: "shiftKey,ctrlKey"
  linter:
    lintOnChange: false
    lintOnChangeInterval: 400
    lintPreviewTabs: false
  "linter-eslint":
    lintHtmlFiles: true
  "linter-php":
    executablePath: "/usr/local/bin/php"
  "linter-phpcs":
    autoExecutableSearch: false
    disableWhenNoConfigFile: true
    executablePath: "~/.composer/vendor/bin/phpcs"
  "linter-phpmd":
    executablePath: "/home/edvin/.config/composer/vendor/bin/phpmd"
    rulesets: "cleancode,codesize,unusedcode"
  "linter-ui-default":
    hidePanelWhenEmpty: true
    panelHeight: 69
  "markdown-preview": {}
  minimap:
    absoluteMode: true
    plugins:
      bookmarks: true
      bookmarksDecorationsZIndex: 0
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      linter: true
      linterDecorationsZIndex: 0
      "minimap-hide": true
      "minimap-hideDecorationsZIndex": 0
      selection: true
      selectionDecorationsZIndex: 0
  "one-dark-ui":
    fontSize: 12
    tabSizing: "Minimum"
  "package-generator":
    packageSyntax: "coffeescript"
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "php-cs-fixer":
    executablePath: "/home/edvin/.config/composer/vendor/bin/php-cs-fixer"
    executeOnSave: true
    pathMode: "intersection"
  "php-integrator-autocomplete-plus": {}
  "php-integrator-base":
    core: {}
    general: {}
    linting:
      showMissingDocs: false
  "php-integrator-base:confirmed-upgrade-message": true
  "php-integrator-linter":
    showMissingDocs: false
  tabs:
    enableVcsColoring: true
  "time-for-break":
    timeBetween: 1
  "tree-view":
    focusOnReveal: false
  "vim-mode":
    useSmartcaseForSearch: true
  "vim-mode-plus":
    stayOnDelete: true
    stayOnTransformString: true
    stayOnYank: true
    useClipboardAsDefaultRegister: true
  wakatime:
    apikey: "Saved in your ~/.wakatime.cfg file"
  "wakatime-hidden":
    lastInit: 1492498417
  welcome:
    showOnStartup: false
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
